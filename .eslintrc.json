{
  "extends": ["standard", "standard-jsx"],
  "plugins": ["jsx-a11y"],
  "globals": {
    "Headers": true,
    "describe": true,
    "it": true,
    "expect": true,
    "process": true,
    "_": true,
    "URLSearchParams": true,
    "location": true,
    "FileReader": true,
    "window": true,
    "localStorage": true,
    "console": true,
    "document": true,
    "Meteor": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": "off",
    "space-before-function-paren": "off",
    "jsx-quotes": ["error", "prefer-double"]
  }
}